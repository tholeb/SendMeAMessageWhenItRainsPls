{
  "name": "client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "prop-types": "^15.7.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.1",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "npm run lint && npm run build:css && react-scripts start",
    "build": "npm run lint && npm run purge:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "purge:css": "cross-env NODE_ENV=production postcss src/assets/styles/tailwind.css -o src/assets/styles/output.css",
    "build:css": "postcss src/assets/styles/tailwind.css -o src/assets/styles/output.css",
    "watch:css": "postcss src/assets/styles/tailwind.css -o src/assets/styles/output.css -w",
    "changelog": "auto-changelog --starting-version 3.0.0 --commit-limit false --hide-credit",
    "lint": "eslint src --ext .js,.jsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "auto-changelog": "^2.3.0",
    "autoprefixer": "^10.3.4",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.8",
    "eslint": "^7.32.0",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-tailwindcss": "^1.14.3",
    "postcss": "^8.3.6",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.2.15"
  }
}
