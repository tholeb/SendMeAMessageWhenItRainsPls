{
  "name": "react-tailwind-starter",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8080/",
  "dependencies": {
    "@headlessui/react": "^1.4.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^3.4.3"
  },
  "scripts": {
    "purge:css": "cross-env NODE_ENV=production postcss src/assets/styles/tailwind.css -o src/assets/styles/index.css",
    "build:css": "postcss src/assets/styles/tailwind.css -o src/assets/styles/index.css",
    "watch:css": "postcss src/assets/styles/tailwind.css -o src/assets/styles/index.css -w",
    "start": "npm run build:css && react-scripts start",
    "build": "npm run purge:css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@heroicons/react": "^1.0.4",
    "auto-changelog": "^2.3.0",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.2",
    "cssnano": "^4.1.10",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.25.1",
    "eslint-plugin-tailwind": "^0.2.1",
    "postcss": "^7.0.36",
    "postcss-cli": "^7.1.1",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.2.14"
  }
}
